#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/yashshrestha/Developer/blog/eleventy/node_modules/.pnpm/@11ty+eleventy-dev-server@2.0.8/node_modules/@11ty/eleventy-dev-server/node_modules:/Users/yashshrestha/Developer/blog/eleventy/node_modules/.pnpm/@11ty+eleventy-dev-server@2.0.8/node_modules/@11ty/node_modules:/Users/yashshrestha/Developer/blog/eleventy/node_modules/.pnpm/@11ty+eleventy-dev-server@2.0.8/node_modules:/Users/yashshrestha/Developer/blog/eleventy/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/yashshrestha/Developer/blog/eleventy/node_modules/.pnpm/@11ty+eleventy-dev-server@2.0.8/node_modules/@11ty/eleventy-dev-server/node_modules:/Users/yashshrestha/Developer/blog/eleventy/node_modules/.pnpm/@11ty+eleventy-dev-server@2.0.8/node_modules/@11ty/node_modules:/Users/yashshrestha/Developer/blog/eleventy/node_modules/.pnpm/@11ty+eleventy-dev-server@2.0.8/node_modules:/Users/yashshrestha/Developer/blog/eleventy/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../@11ty/eleventy-dev-server/cmd.js" "$@"
else
  exec node  "$basedir/../@11ty/eleventy-dev-server/cmd.js" "$@"
fi
